for url in urls:
    page = urllib.urlopen(url)
    html = page.read() 
    soup=BeautifulSoup(html)
    links=soup.find("div",{'id':'content'})
    left= links.find("div",{'class':'left'})
    left1=left.findAll("div",{'class':'articles'})
    articles= left1[0].findAll("a",href=True)
    print articles
    #for link in articles:
