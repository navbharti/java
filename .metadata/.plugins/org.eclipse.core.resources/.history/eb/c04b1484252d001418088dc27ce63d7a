from bs4 import BeautifulSoup
import urllib
import re

page = urllib.urlopen("http://pincode.net.in/ANDAMAN_AND_NICOBAR_ISLANDS/NICOBAR/C/")
html = page.read() 
soup=BeautifulSoup(html)

links=soup.find("div",{'id':'content'})
left= links.find("div",{'class':'left'})
print left