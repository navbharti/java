from bs4 import BeautifulSoup
import urllib

page = urllib.urlopen("http://pincode.net.in/ANDAMAN_AND_NICOBAR_ISLANDS/NICOBAR/C/")
html = page.read() 
soup=BeautifulSoup(html)

body=soup.find('table')
#print body
heading= body.find('div',{'class','article'})
print heading